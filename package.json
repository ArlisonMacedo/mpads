{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex --knexfile ./src/knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile ./src/knexfile.ts seed:run",
    "knex:migrate:prod": "knex --knexfile ./dist/knexfile.js migrate:latest",
    "knex:seed:prod": "knex --knexfile ./dist/knexfile.js seed:run",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.21",
    "multer": "^1.4.3",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
